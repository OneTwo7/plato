"use strict";
var lessonId = "<%= @id %>";
var pathId = "<%= @path_id %>";
var fId = "<%= @first_lesson.id %>";
var $link = $("a[href='/paths/" + pathId + "?lesson_id=" + lessonId + "']");
var $new = $("a[href='/paths/" + pathId + "?lesson_id=" + fId + "']");
if ($link) {
  $link.remove();
}
if ($new) {
  $new.addClass("active");
}
var $lesson = $("<%= escape_javascript(render 'lesson', lesson: @first_lesson) %>");
window.prepareLesson($lesson);
$("#lesson").replaceWith($lesson);